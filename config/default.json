{
  "host": "localhost",
  "port": 14444,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "nedb": "../data",
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://berviantoleo.herokuapp.com",
      "subject": "anonymous",
      "issuer": "bervLeo",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  },
  "postgres": "postgres://postgres:@localhost:5432/my_personal_web_api"
}
